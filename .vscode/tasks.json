{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build:dev",
			"dependsOn": ["install", "kill-port"],
			"dependsOrder": "sequence"
		},
		{
			"label": "install",
			"command": "yarn",
			"group": "build",
			"type": "shell"
		},
		{
			"label": "kill-port",
			"command": "npx",
			"args": ["kill-port", "3000"],
			"group": "build",
			"type": "shell"
		}
	]
}
